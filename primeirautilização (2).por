programa {
  funcao inicio() {
    cadeia nome, idade, aniversario
    
    escreva ("Insira seu nome: ")
    leia(nome)
    escreva ("Digite sua idade: " )
    leia(idade)
    escreva ("Digite sua data de nascimento:")
    leia(aniversario)
    
    escreva ("Você se chama ", nome, ", tem ", idade, " anos e nasceu em ", aniversario )
  }
}
